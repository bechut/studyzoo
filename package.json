{
  "name": "studynx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "gen-app": "cd apps && nx g @nx/nest:app",
    "gen-package": "cd packages && nx g @nx/nest:lib",
    "serve-test": "nx run test:serve:production",
    "serve-test-ms": "prisma generate --schema=apps/test-ms/prisma/schema.prisma && nx run test-ms:serve:production",
    "serve-user-ms": "prisma generate --schema=apps/user-ms/prisma/schema.prisma && nx run user-ms:serve:production",
    "gen-mc-ms-user": "cd apps/user-ms/src/app && nest g mo && nest g co",
    "serve-session-ms": "prisma generate --schema=apps/session-ms/prisma/schema.prisma && nx run session-ms:serve:production",
    "gen-mc-ms-session": "cd apps/session-ms/src/app && nest g mo && nest g co",
    "serve-msapp": "nx run msapp:serve:production",
    "gen-mc-msapp": "cd apps/msapp/src/app && nest g mo && nest g co"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint-plugin": "16.5.5",
    "@nx/jest": "16.5.5",
    "@nx/js": "16.5.5",
    "@nx/linter": "16.5.5",
    "@nx/nest": "^16.5.5",
    "@nx/node": "16.5.5",
    "@nx/webpack": "16.5.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.196",
    "@types/node": "~18.7.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "16.5.5",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.1.2",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.0.0",
    "axios": "^1.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jest-mock-extended": "^3.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  }
}
